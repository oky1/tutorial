
<!DOCTYPE html>
<html lang="en" data-ng-app='myApp'>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Navbar Template for Bootstrap</title>
    <link href="libs/bootstrap-css/bootstrap.min.css" rel="stylesheet">
      <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <div class="container">
      <!-- Static navbar -->
      <nav class="navbar navbar-default" data-ng-controller='navCtrl'>
        <div class="container-fluid">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">oky1</a>
          </div>
          <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
              <li data-ng-class="{active: isActive('/home')}"><a href="/home">Home</a></li>
              <li data-ng-class="{active: isActive('/about')}"><a href="/about">About</a></li>
              <li data-ng-class="{active: isActive('/contact')}"><a href="/contact">Contact</a></li>
            </ul>
            </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
      </nav>
     <div class='container'>
         <div ng-view></div>
     </div>
    </div> <!-- /container -->
    <script src="libs/jquery/jquery.js"></script>
    <script src="libs/bootstrap/bootstrap.js"></script>
    <script src='libs/angular/angular.js'></script>
    <script src='libs/angular-route/angular-route.js'></script>
    <script src='libs/angular-bootstrap/ui-bootstrap-tpls.js'></script>
    <script src="js/app.js"></script> 
    <script src='js/controllers/navCtrl.js'></script>
    <script src='js/controllers/homeCtrl.js'></script>
    <script src='js/controllers/contactCtrl.js'></script>
    <script src='js/controllers/aboutCtrl.js'></script>
</body>
</html>
